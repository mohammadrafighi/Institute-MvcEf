@model Student
@{
    ViewData["Title"] = "Login";
}

<h2>Login</h2>

<form method="post">
    <label>Username</label>
    <input class="form-control" name="Username" />

    <label>Password</label>
    <input class="form-control" type="password" name="Password" />

    <button class="btn btn-primary mt-2">Login</button>
</form>
